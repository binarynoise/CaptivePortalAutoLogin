{{<base-layout}}
    {{$content}}
        <p><a href="har-upload" class="button">Upload HAR File</a></p>
        <table>
            <thead>
            <tr>
                <th>Domain</th>
                <th>Count</th>
                <th>Details</th>
            </tr>
            </thead>
            <tbody>
            {{#groups}}
                <tr class="mono">
                    <td>{{domain}}</td>
                    <td>{{count}}</td>
                    <td>
                        <a href="group/{{domain}}/">
                            <button type="button">open</button>
                        </a>
                    </td>
                </tr>
            {{/groups}}
            </tbody>
        </table>
    {{/content}}
{{/base-layout}}
